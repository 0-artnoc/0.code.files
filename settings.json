{
  "workbench.startupEditor": "none",
  "zenMode.hideTabs": false,
  "diffEditor.renderSideBySide": true,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "type",
  "search.location": "panel",
  "search.useReplacePreview": false,
  "search.useIgnoreFiles": true,
  "search.showLineNumbers": true,
  "editor.folding": false,
  "editor.selectionHighlight": false,
  "editor.wordBasedSuggestions": false,
  "editor.snippetSuggestions": "none",
  "editor.suggestOnTriggerCharacters": false,
  "editor.quickSuggestionsDelay": 1000,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.parameterHints.enabled": false,
  "editor.cursorBlinking": "solid",
  "editor.fontLigatures": true,
  "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
  "editor.minimap.enabled": false,
  "editor.matchBrackets": true,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.renderIndentGuides": false,
  "editor.autoIndent": true,
  "editor.tabSize": 2,
  "editor.renderLineHighlight": "none",
  "editor.insertSpaces": true,
  "editor.find.autoFindInSelection": "always",
  "editor.overviewRulerBorder": false,
  "window.restoreWindows": "none",
  "window.titleBarStyle": "custom",
  "window.title": "${activeEditorMedium}",
  "terminal.integrated.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
  "terminal.integrated.fontWeightBold": "normal",
  "git.decorations.enabled": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.hotExit": "off",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*/__pycache__": true,
    "**/node_modules": true,
    "**/target/debug": true,
    "**/target/rls": true,
    "**/.rustup": true,
    "**/.classpath": true,
    "**/*.class": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "search.exclude": {
    "**/__pycache__": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/*.map": true,
    "**/bower_components": true,
    "**/ios/build": true,
    "**/node_modules": true,
    "**/private": true,
    "**/target/debug": true,
    "**/target/rls": true,
    "**/.rustup": true,
    "**/typings": true,
    "**/var": true
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.autoClosingBrackets": "always"
  },
  "[java]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.autoClosingBrackets": "always"
  },
  "[cpp]": {
    "editor.tabSize": 2,
    "editor.autoClosingBrackets": "always"
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.autoClosingBrackets": "always"
  },
  "[go]": {
    "editor.tabSize": 4,
    "editor.autoClosingBrackets": "always"
  },
  "[c]": {
    "editor.tabSize": 2,
    "editor.autoClosingBrackets": "always"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "breadcrumbs.enabled": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "window.menuBarVisibility": "toggle",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.restoreViewState": false,
  "workbench.statusBar.visible": true,
  "workbench.commandPalette.history": 5,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "extensions.ignoreRecommendations": false,
  "git-graph.showStatusBarItem": false,
  "git-graph.showUncommittedChanges": false,
  "git-graph.graphStyle": "angular",
  "vim.easymotion": false,
  "vim.leader": ",",
  "vim.useSystemClipboard": true,
  "vim.history": 1,
  "vim.useCtrlKeys": false,
  "git.untrackedChanges": "separate",
  "editor.suggest.showSnippets": false,
  "editor.suggestSelection": "first",
  "editor.lightbulb.enabled": false,
  "editor.occurrencesHighlight": false,
  "workbench.colorTheme": "GitHub Sharp Dark",
  "problems.showCurrentInStatus": true,
  "terminal.integrated.rendererType": "experimentalWebgl",
  "workbench.iconTheme": "vscode-icons",
  "explorer.decorations.colors": false,
  "explorer.autoReveal": false,
}
